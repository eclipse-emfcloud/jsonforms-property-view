{
  "private": true,
  "name": "browser-app",
  "license": "(EPL-2.0 OR MIT)",
  "version": "0.8.0-theia-cr01",
  "dependencies": {
    "@eclipse-emfcloud/person-detail-property-view": "^0.8.0-theia-cr01",
    "@theia/core": "^1.0.0",
    "@theia/editor": "^1.0.0",
    "@theia/filesystem": "^1.0.0",
    "@theia/markers": "^1.0.0",
    "@theia/messages": "^1.0.0",
    "@theia/monaco": "^1.0.0",
    "@theia/navigator": "^1.0.0",
    "@theia/plugin-ext-vscode": "^1.0.0",
    "@theia/preferences": "^1.0.0",
    "@theia/process": "^1.0.0",
    "@theia/property-view": "^1.0.0",
    "@theia/terminal": "^1.0.0",
    "@theia/workspace": "^1.0.0"
  },
  "devDependencies": {
    "@theia/cli": "^1.0.0"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn build",
    "download:plugins": "theia download:plugins",
    "clean": "theia clean",
    "build": "theia build --mode development && yarn download:plugins",
    "start": "theia start --plugins=local-dir:./plugins --root-dir=../workspace",
    "watch": "theia build --watch --mode development"
  },
  "theia": {
    "frontend": {
      "config": {
        "applicationName": "jsonforms-property-view Browser Example"
      }
    }
  },
  "theiaPluginsDir": "plugins",
  "theiaPlugins": {
    "vscode-builtin-json": "https://open-vsx.org/api/vscode/json/1.62.3/file/vscode.json-1.62.3.vsix",
    "vscode-builtin-json-language-features": "https://open-vsx.org/api/vscode/json-language-features/1.62.3/file/vscode.json-language-features-1.62.3.vsix"
  }
}
