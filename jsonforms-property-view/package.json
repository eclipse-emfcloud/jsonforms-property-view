  ],
  "author": "EclipseSource",
  "contributors": [
    {
      "name": "Nina Doschek",
      "email": "ndoschek@eclipsesource.com",
      "url": "https://www.eclipsesource.com"
    }
  ],
  "homepage": "https://www.eclipse.org/emfcloud/",
  "bugs": "https://github.com/eclipse-emfcloud/jsonforms-property-view/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse-emfcloud/jsonforms-property-view.git"
  },
  "dependencies": {
    "@theia/core": "^1.0.0",
    "@theia/filesystem": "^1.0.0",
    "@theia/property-view": "^1.0.0",
